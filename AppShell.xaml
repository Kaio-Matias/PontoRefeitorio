<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="PontoRefeitorio.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:PontoRefeitorio.Views"
    xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
    Shell.FlyoutBehavior="Disabled"
    Title="PontoRefeitorio">

    <Shell.Behaviors>
        <toolkit:StatusBarBehavior StatusBarColor="{StaticResource White}" StatusBarStyle="DarkContent" />
    </Shell.Behaviors>
    <ShellContent
        Title="Login"
        ContentTemplate="{DataTemplate local:Apresentacao}"
        Route="Apresentacao" />

</Shell>
